{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "https://toolbox.com.ng/api/v1/",
        "API_URL": "https://toolbox.com.ng/api/v1/",
        "REQUEST_TIMEOUT": "60000"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "ANDROID_SDK_ROOT": "/Users/mac/Library/Android/sdk"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
